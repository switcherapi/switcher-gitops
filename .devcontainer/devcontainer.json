{
	"name": "Switcher GitOps Dev Container",
	"dockerComposeFile": [
		"docker-compose.yml"
	],
	"service": "switcher-gitops",
	"workspaceFolder": "/workspace",
	"customizations": {
		"vscode": {
			"extensions": [
				"golang.go"
			],
			"settings": {
				"terminal.integrated.shell.linux": "/bin/bash"
			}
		}
	}
}